<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
<!--////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////-->
    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="600dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="15dp"
        android:layout_marginStart="10dp"
        android:layout_marginEnd="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="10dp"
        app:cardCornerRadius="10dp"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="204dp" >
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/backbroad">
            <TextView
                android:id="@+id/text_E"
                android:text="@string/carbinet_1"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_marginTop="18dp"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_marginStart="10dp"
                app:layout_constraintEnd_toEndOf="parent"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/font"
                android:textSize="18sp"
                />
            <TextView
                android:id="@+id/text_location_rcv_name"
                android:text="@string/Location"
                app:layout_constraintTop_toBottomOf="@+id/text_E"
                android:layout_marginTop="10dp"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_marginStart="10dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/font"
                android:textSize="18sp"
                />
            <TextView
                android:id="@+id/show_location_rcv_name"
                android:text="@string/showLocation"
                app:layout_constraintTop_toTopOf="@id/text_location_rcv_name"
                app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginEnd="10dp"
                android:layout_width="90dp"
                android:layout_height="wrap_content"
                android:gravity="end"
                android:textColor="@color/purple_700"
                android:textSize="18sp"
                />
            <TextView
                android:id="@+id/text_row_rcv_name"
                android:text="@string/row"
                app:layout_constraintTop_toBottomOf="@id/text_location_rcv_name"
                android:layout_marginTop="10dp"
                app:layout_constraintStart_toStartOf="@id/text_location_rcv_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/font"
                android:textSize="18sp"
                />
            <TextView
                android:id="@+id/show_row_rcv_name"
                app:layout_constraintTop_toTopOf="@id/text_row_rcv_name"
                app:layout_constraintEnd_toEndOf="@id/show_location_rcv_name"
                android:layout_width="50dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:textColor="@color/purple_700"
                android:textSize="18sp"
                />
            <TextView
                android:id="@+id/text_column_rcv_name"
                android:text="@string/column"
                app:layout_constraintTop_toBottomOf="@id/text_row_rcv_name"
                android:layout_marginTop="10dp"
                app:layout_constraintStart_toStartOf="@id/text_location_rcv_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/font"
                android:textSize="18sp"
                />
            <TextView
                android:id="@+id/show_column_rcv_name"
                android:layout_width="50dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:textColor="@color/purple_700"
                android:textSize="18sp"
                app:layout_constraintEnd_toEndOf="@id/show_location_rcv_name"
                app:layout_constraintTop_toTopOf="@id/text_column_rcv_name"
                />
            <TextView
                android:id="@+id/textH"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:text="@string/text_H"
                android:textColor="@color/font"
                android:textSize="18sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="@id/text_location_rcv_name"
                app:layout_constraintTop_toBottomOf="@id/text_column_rcv_name"
                />
            <TextView
                android:id="@+id/text_location_rcv_name2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:layout_marginTop="10dp"
                android:text="@string/Location"
                android:textColor="@color/font"
                android:textSize="18sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textH"
                />
            <TextView
                android:id="@+id/show_location_rcv_name2"
                android:text="@string/show_text_location"
                app:layout_constraintTop_toTopOf="@id/text_location_rcv_name2"
                app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginEnd="10dp"
                android:layout_width="90dp"
                android:layout_height="wrap_content"
                android:gravity="end"
                android:textColor="@color/purple_700"
                android:textSize="18sp"
                />
            <TextView
                android:id="@+id/text_row_rcv_nameH"
                android:text="@string/row"
                app:layout_constraintTop_toBottomOf="@id/text_location_rcv_name2"
                android:layout_marginTop="10dp"
                app:layout_constraintStart_toStartOf="@+id/text_location_rcv_name2"
                android:layout_marginStart="0dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/font"
                android:textSize="18sp"
                />
            <TextView
                android:id="@+id/show_row_rcv_nameH"
                app:layout_constraintTop_toTopOf="@id/text_row_rcv_nameH"
                app:layout_constraintEnd_toEndOf="@id/show_location_rcv_name"
                android:layout_width="50dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:textColor="@color/purple_700"
                android:textSize="18sp"
                />
            <TextView
                android:id="@+id/text_column_rcv_name2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:text="@string/column"
                android:textColor="@color/font"
                android:textSize="18sp"
                app:layout_constraintStart_toStartOf="@id/text_location_rcv_name"
                app:layout_constraintTop_toBottomOf="@+id/text_row_rcv_nameH"
                />
            <TextView
                android:id="@+id/show_column_rcv_name2"
                android:layout_width="50dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:textColor="@color/purple_700"
                android:textSize="18sp"
                app:layout_constraintEnd_toEndOf="@id/show_location_rcv_name"
                app:layout_constraintTop_toTopOf="@+id/text_column_rcv_name2"
                />
            <TextView
                android:id="@+id/textH2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:text="@string/text_TC1"
                android:textColor="@color/font"
                android:textSize="18sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="@id/text_location_rcv_name"
                app:layout_constraintTop_toBottomOf="@+id/text_column_rcv_name2"
                />
            <TextView
                android:id="@+id/text_location_rcv_name3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:text="@string/Location"
                android:textColor="@color/font"
                android:textSize="18sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textH2"
                />
            <TextView
                android:id="@+id/show_location_rcv_name3"
                app:layout_constraintTop_toTopOf="@+id/text_location_rcv_name3"
                android:layout_marginEnd="10dp"
                app:layout_constraintEnd_toEndOf="parent"
                android:layout_width="90dp"
                android:layout_height="wrap_content"
                android:gravity="end"
                android:textColor="@color/purple_700"
                android:textSize="18sp"
                />
            <TextView
                android:id="@+id/text_row_rcv_nameTC1"
                android:text="@string/row"
                app:layout_constraintTop_toBottomOf="@+id/text_location_rcv_name3"
                android:layout_marginTop="10dp"
                app:layout_constraintStart_toStartOf="@+id/text_location_rcv_name3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/font"
                android:textSize="18sp"
                />
            <TextView
                android:id="@+id/show_row_rcv_nameTC1"
                app:layout_constraintTop_toTopOf="@id/text_row_rcv_nameTC1"
                app:layout_constraintEnd_toEndOf="@id/show_location_rcv_name"
                android:layout_width="50dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:textColor="@color/purple_700"
                android:textSize="18sp"
                />
            <TextView
                android:id="@+id/text_column_rcv_nameTC1"
                android:text="@string/column"
                app:layout_constraintTop_toBottomOf="@+id/text_row_rcv_nameTC1"
                android:layout_marginTop="10dp"
                app:layout_constraintStart_toStartOf="@id/text_location_rcv_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/font"
                android:textSize="18sp"
                />
            <TextView
                android:id="@+id/show_column_rcv_nameTC1"
                android:layout_width="50dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:textColor="@color/purple_700"
                android:textSize="18sp"
                app:layout_constraintEnd_toEndOf="@id/show_location_rcv_name"
                app:layout_constraintTop_toTopOf="@+id/text_column_rcv_nameTC1"
                />
            <TextView
                android:id="@+id/text_TC2"
                android:text="@string/text_TC2"
                app:layout_constraintTop_toBottomOf="@+id/text_column_rcv_nameTC1"
                android:layout_marginTop="10dp"
                app:layout_constraintStart_toStartOf="@id/text_location_rcv_name"
                app:layout_constraintEnd_toEndOf="parent"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/font"
                android:textSize="18sp"
                />
            <TextView
                android:id="@+id/text_location_rcv_nameTC2"
                android:text="@string/Location"
                app:layout_constraintTop_toBottomOf="@+id/text_TC2"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_marginStart="10dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/font"
                android:textSize="18sp"
                />
            <TextView
                android:id="@+id/show_location_rcv_nameTC2"
                app:layout_constraintTop_toTopOf="@+id/text_location_rcv_nameTC2"
                android:layout_marginEnd="10dp"
                app:layout_constraintEnd_toEndOf="parent"
                android:layout_width="90dp"
                android:layout_height="wrap_content"
                android:gravity="end"
                android:textColor="@color/purple_700"
                android:textSize="18sp"
                />
            <TextView
                android:id="@+id/text_row_rcv_nameTC2"
                android:text="@string/row"
                app:layout_constraintTop_toBottomOf="@+id/text_location_rcv_nameTC2"
                android:layout_marginTop="10dp"
                app:layout_constraintStart_toStartOf="@+id/text_location_rcv_name3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/font"
                android:textSize="18sp"
                />
            <TextView
                android:id="@+id/show_row_rcv_nameTC2"
                app:layout_constraintTop_toTopOf="@id/text_row_rcv_nameTC2"
                app:layout_constraintEnd_toEndOf="@id/show_location_rcv_name"
                android:layout_width="50dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:textColor="@color/purple_700"
                android:textSize="18sp"
                />
            <TextView
                android:id="@+id/text_column_rcv_nameTC2"
                android:text="@string/column"
                app:layout_constraintTop_toBottomOf="@+id/text_row_rcv_nameTC2"
                android:layout_marginTop="10dp"
                app:layout_constraintStart_toStartOf="@id/text_location_rcv_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/font"
                android:textSize="18sp"
                />
            <TextView
                android:id="@+id/show_column_rcv_nameTC2"
                android:layout_width="50dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:textColor="@color/purple_700"
                android:textSize="18sp"
                app:layout_constraintEnd_toEndOf="@id/show_location_rcv_name"
                app:layout_constraintTop_toTopOf="@+id/text_column_rcv_nameTC2"
                />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.cardview.widget.CardView>
</androidx.constraintlayout.widget.ConstraintLayout>